fn getchar() -> i64;
fn putchar(c: i64) -> i64;

fn main() -> i64 {
    let c: i64 = getchar();

    while c != 10 {
        if c >= 97 && c <= 122 {
            putchar(c - 32);
        } else {
            if c >= 65 && c <= 90 {
                putchar(c + 32);
            } else {
                putchar(c);
            }
        }
        c = getchar();
    }

    putchar(10);

    0
}
