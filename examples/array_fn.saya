fn create_array() -> [i64; 3] {
    [10, 20, 30]
}

fn sum_array(arr: [i64; 3]) -> i64 {
    arr[0] + arr[1] + arr[2]
}

fn modify_array(arr: [i64; 3]) -> [i64; 3] {
    arr[0] = 99;
    arr
}

fn main() -> i64 {
    let arr1: [i64; 3] = create_array();
    let total: i64 = sum_array(arr1);

    let arr2: [i64; 3] = modify_array(arr1);
    let result: i64 = arr2[0] + total;

    result
}
